<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering
  SYSTEM "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">

<!-- See also -->
<!-- https://docs.jboss.org/tools/latest/en/hibernatetools/html/reverseengineering.html -->

<hibernate-reverse-engineering>
  <type-mapping>
    <sql-type jdbc-type="CHAR" hibernate-type="java.lang.String"/>
    <sql-type jdbc-type="DATE" hibernate-type="java.time.LocalDate"/>
    <sql-type jdbc-type="TIME" hibernate-type="java.time.LocalTime"/>
    <sql-type jdbc-type="TIMESTAMP" hibernate-type="java.time.LocalDateTime"/>
  </type-mapping>

  <table-filter match-name="youtube_channel" package="com.iskwhdys.newlives.domain.youtube"/>
  <table-filter match-name="youtube_video" package="com.iskwhdys.newlives.domain.youtube"/>
  <table-filter match-name="liver" package="com.iskwhdys.newlives.domain.liver"/>
  <table-filter match-name="liver_tag" package="com.iskwhdys.newlives.domain.liver"/>

  <table-filter match-name="top_live" package="com.iskwhdys.newlives.domain.delivery"/>
  <table-filter match-name="top_upload" package="com.iskwhdys.newlives.domain.delivery"/>
  <table-filter match-name="top_archive" package="com.iskwhdys.newlives.domain.delivery"/>
  <table-filter match-name="top_premier" package="com.iskwhdys.newlives.domain.delivery"/>
  <table-filter match-name="top_schedule" package="com.iskwhdys.newlives.domain.delivery"/>

</hibernate-reverse-engineering>
